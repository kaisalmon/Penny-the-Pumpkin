pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
#include levels.p8

function whoami(val)
    for k,v in pairs(_ENV) do
        if v==val then
            return k
        end
    end
end

function _init()
    for level in all(levels) do
        local name = whoami(level)
        print(name)
        for i, block in ipairs(level) do
            block = preload(block)
            local update = ""
            if block.update then
                update = whoami(block.update) or '???'
            end
            print(" "..i..": "..update)
        end 
    end
end
